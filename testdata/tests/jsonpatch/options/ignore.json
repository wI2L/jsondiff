[{
    "name": "ignore updated elements of a array",
    "before": [
        "a", "b", "c", "d", "e"
    ],
    "after": [
        "a", "x", "c", "y", "z"
    ],
    "ignores": [
        "/1",
        "/3"
    ],
    "patch": [
        { "op": "replace", "path": "/1", "value": "x" },
        { "op": "replace", "path": "/3", "value": "y" },
        { "op": "replace", "path": "/4", "value": "z" }
    ],
    "partial_patch": [
        { "op": "replace", "path": "/4", "value": "z" }
    ]
}, {
    "name": "ignore added keys of a array",
