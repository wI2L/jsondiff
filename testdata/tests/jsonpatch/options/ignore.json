    "ignores": [
        "/4"
    ],
    "patch": [
        { "op": "remove", "path": "/3" },
        { "op": "remove", "path": "/3" }
    ],
    "partial_patch": [
        { "op": "remove", "path": "/3" }
    ]
},{
