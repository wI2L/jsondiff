        "a", "b", "c"
    ],
    "after": [
        "a", "b", "c", "d", "e"
    ],
    "ignores": [
        "/3"
    ],
    "patch": [
        { "op": "add", "path": "/-", "value": "d" },
        { "op": "add", "path": "/-", "value": "e" }
